# OLLAMA_MODEL_PREF='llama3.2'
# OLLAMA_MODEL_TOKEN_LIMIT=4096

# --- Azure OpenAI ---
# LLM_PROVIDER='azure'
# AZURE_OPENAI_ENDPOINT=
# AZURE_OPENAI_KEY=
# OPEN_MODEL_PREF='my-gpt4-deployment'

# --- Gemini ---
# LLM_PROVIDER='gemini'
# GEMINI_API_KEY=
