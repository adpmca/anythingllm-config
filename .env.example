# --- Serper.dev ---
# AGENT_SERPER_DEV_KEY=

# --- Bing Search ---
# AGENT_BING_SEARCH_API_KEY=

# --- Google Custom Search ---
# AGENT_GSE_KEY=
# AGENT_GSE_CTX=

###########################################
######## MISC / SECURITY ##################
###########################################

# Disable telemetry (optional)
# DISABLE_TELEMETRY="true"

# Disable Swagger API docs in production (recommended)
# DISABLE_SWAGGER_DOCS="true"

# Password complexity requirements
# PASSWORDMINCHAR=8
# PASSWORDMAXCHAR=250
# PASSWORDLOWERCASE=1
# PASSWORDUPPERCASE=1
# PASSWORDNUMERIC=1
# PASSWORDSYMBOL=1
