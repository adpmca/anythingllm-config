SERVER_PORT=3001
STORAGE_DIR="/app/server/storage"
UID='1000'
GID='1000'

# Authentication â€” uncomment and set a strong password when hosting remotely
# AUTH_TOKEN="change-me-to-a-strong-password"
# JWT_SECRET="change-me-to-a-random-32-char-string"

###########################################
######## LLM PROVIDER SELECTION ###########
###########################################

# OpenAI (default)
LLM_PROVIDER='openai'
OPEN_AI_KEY=
OPEN_MODEL_PREF='gpt-4o'

# Uncomment ONE alternative provider block below if not using OpenAI

# --- Anthropic ---
# LLM_PROVIDER='anthropic'
